{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },

  "Clients": [
    {
      "ClientId": "implicit-client",
      "ClientName": "Implicit Sample Client",

      "AllowedGrantTypes": [ "implicit" ],
      "AllowedScopes": [ "openid", "profile", "people-api" ],
      "AllowAccessTokensViaBrowser": true,

      "RedirectUris": [
        "https://localhost:5021/"
      ],
      "FrontChannelLogoutUri": "https://localhost:5021/signout.html",
      "PostLogoutRedirectUris": [ "https://localhost:5021/" ],
      "AllowedCorsOrigins": [ "https://localhost:5021" ]
    }
  ]
}
